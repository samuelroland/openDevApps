{
  "manifest_version": 2,
  "name": "openDevD",
  "version": "0.1",
  "homepage_url": "https://github.com/samuelroland/openDevD",
  "description": "Open quickly in the browser some local or remote dev apps.",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "19": "icons/16.png",
    "48": "icons/48.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Show links of openDevD",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "salutcoolahahah@example.com",
      "strict_min_version": "42.0"
    }
  }
}