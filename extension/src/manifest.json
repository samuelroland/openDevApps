{
  "manifest_version": 2,
  "name": "openDevD",
  "version": "0.1",
  "author": "Samuel Roland",
  "homepage_url": "https://github.com/samuelroland/openDevD",
  "description": "Access quickly to local or remote development-related applications.",
  "default_locale": "en",
  "permissions": ["storage"],
  "icons": {
    "19": "icons/16.png",
    "48": "icons/48.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Show links of openDevD",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "testaaaa@example.com",
      "strict_min_version": "42.0"
    }
  }
}
