{
    "manifest_version": 2,
    "name": "openDevD",
    "version": "0.1",
    "description": "Create and access to a list of favorite links for dev, with additionnal support for local apps.",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/icon-48.png",
        "default_title": "Show links of openDevD",
        "default_popup": "popup/popup.html"
    }
}